                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/lunch26Timings"
                    app:layout_constraintStart_toStartOf="@+id/lunch26Guideline1"
                    app:layout_constraintBottom_toBottomOf="@+id/lunch26Constraint"
                    android:gravity="center"
                    android:textColor="#838383"
                    android:textSize="14sp"
                    android:text="@string/Menu_Click_Here"
                    android:onClick="menuClickEvent"
                    android:layout_marginBottom="16dp"/>

                <TextView
                    android:id="@+id/lunch26Venue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    app:layout_constraintTop_toBottomOf="@id/lunch26BottomLine"
                    app:layout_constraintEnd_toEndOf="@+id/lunch26Guideline2"
                    android:gravity="center"
                    android:textColor="#838383"
                    android:textSize="14sp"
                    android:text="@string/Lunch_26_venue"
                    android:onClick="venueClickEvent"/>

                <androidx.constraintlayout.widget.Group
                    android:id="@+id/lunch26CardGroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:constraint_referenced_ids="lunch26BottomLine,lunch26Contents,lunch26DressCode,lunch26UpperLine,lunch26Timings,lunch26Image, lunch26Venue"
                    android:visibility="gone"/>




            </androidx.constraintlayout.widget.ConstraintLayout>




        </androidx.cardview.widget.CardView>



    </LinearLayout>
</ScrollView>