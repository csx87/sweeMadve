<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <!-- Breakfast 26 -->
        <androidx.cardview.widget.CardView
            android:id="@+id/breakfast26"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="10dp"
            app:cardCornerRadius="5dp"
            app:strokeColor="@color/teal_200"
            app:strokeWidth="2dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/breakfast26Constraint">

                <View
                    android:id="@+id/breakfast26ClickView"
                    android:layout_width="368dp"
                    android:layout_height="49dp"
                    app:layout_constraintEnd_toEndOf="@id/breakfast26Constraint"
                    app:layout_constraintStart_toStartOf="@+id/breakfast26Constraint"
                    app:layout_constraintTop_toTopOf="@id/breakfast26Constraint"/>

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/breakfast26Guideline1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_begin="16dp" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/breakfast26Guideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_end="16dp" />


                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/breakfast26VenueGuideline"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_begin="145dp" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/breakfast26MidGuideline"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_begin="255dp" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/breakfast26Guideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_begin="16dp" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/breakfast26HeaderGuideline"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_begin="8dp" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/breakfast26Guideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_end="16dp" />


                <TextView
                    android:id="@+id/breakfast26CardHeader"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Breakfast_26_Header"
                    android:textColor="#00255E"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toTopOf="@+id/breakfast26Guideline4"
                    app:layout_constraintStart_toStartOf="@+id/breakfast26Guideline1"
                    app:layout_constraintTop_toTopOf="@+id/breakfast26HeaderGuideline"
                    app:layout_constraintVertical_bias="0.0" />

                <ImageView
                    android:id="@+id/breakfast26Arrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="4dp"
                    android:padding="4dp"
                    app:layout_constraintBottom_toBottomOf="@+id/breakfast26CardHeader"
                    app:layout_constraintEnd_toStartOf="@+id/breakfast26Guideline2"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/breakfast26CardHeader"
                    app:layout_constraintTop_toTopOf="@+id/breakfast26CardHeader"
                    app:srcCompat="@android:drawable/arrow_down_float"
                    app:tint="@color/teal_200"/>

                <TextView
                    android:id="@+id/breakfast26UpperTimings"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/breakfast26CardHeader"
                    app:layout_constraintStart_toStartOf="@+id/breakfast26Guideline1"
                    android:textSize="12sp"
                    android:text="@string/Breakfast_26_timings"
                    android:textColor="#838383"/>

                <TextView
                    android:id="@+id/breakfast26UpperVenue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/breakfast26CardHeader"
                    app:layout_constraintStart_toEndOf="@+id/breakfast26VenueGuideline"
                    app:layout_constraintBottom_toBottomOf="@id/breakfast26Constraint"
                    android:textSize="12sp"
                    android:text="@string/Breakfast_26_Upper_Venue"
                    android:textColor="#838383"
                    android:layout_marginBottom="8dp"

                    />

                <View
                    android:id="@+id/breakfast26UpperLine"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="16dp"
                    android:background="#CECECE"
                    app:layout_constraintEnd_toStartOf="@+id/breakfast26Guideline2"
                    app:layout_constraintStart_toStartOf="@+id/breakfast26Guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/breakfast26CardHeader" />

                <TextView
                    android:id="@+id/breakfast26Contents"
                    android:layout_width="238dp"
                    android:layout_height="118dp"
                    android:layout_marginTop="16dp"
                    android:text="@string/Breakfast_26_Contents"
                    android:textColor="#838383"
                    android:textSize="12sp"
                    app:layout_constraintBottom_toBottomOf="@id/breakfast26BottomLine"
                    app:layout_constraintEnd_toStartOf="@+id/breakfast26MidGuideline"
                    app:layout_constraintStart_toStartOf="@id/breakfast26Guideline1"
                    app:layout_constraintTop_toBottomOf="@id/breakfast26UpperLine"
                    />

                <ImageView
                    android:id="@+id/breakfast26Image"
                    android:layout_width="99dp"
                    android:layout_height="93dp"
                    app:layout_constraintEnd_toStartOf="@id/breakfast26Guideline2"
                    app:layout_constraintStart_toStartOf="@+id/breakfast26MidGuideline"
                    app:layout_constraintTop_toBottomOf="@id/breakfast26UpperLine"
                    android:layout_marginTop="16dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="5dp"
                    app:srcCompat="@drawable/lunch"
                    />

                <View
                    android:id="@+id/breakfast26BottomLine"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_marginTop="16dp"
                    android:background="#CECECE"
                    app:layout_constraintEnd_toStartOf="@+id/breakfast26Guideline2"
                    app:layout_constraintStart_toStartOf="@+id/breakfast26Guideline1"
                    app:layout_constraintTop_toBottomOf="@+id/breakfast26Image"/>

                <TextView
                    android:id="@+id/breakfast26Timings"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    app:layout_constraintTop_toBottomOf="@id/breakfast26BottomLine"
                    app:layout_constraintStart_toStartOf="@+id/breakfast26Guideline1"
                    android:gravity="center"
                    android:textColor="#838383"
                    android:textSize="14sp"
                    android:text="@string/Breakfast_26_timings"/>

                <TextView
                    android:id="@+id/breakfast26DressCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/breakfast26Timings"
                    app:layout_constraintStart_toStartOf="@+id/breakfast26Guideline1"
                    app:layout_constraintBottom_toBottomOf="@+id/breakfast26Constraint"
                    android:gravity="center"
                    android:textColor="#838383"
                    android:textSize="14sp"
                    android:text="@string/Menu_Click_Here"
                    android:onClick="menuClickEvent"
                    android:layout_marginBottom="16dp"/>

                <TextView
                    android:id="@+id/breakfast26Venue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    app:layout_constraintTop_toBottomOf="@id/breakfast26BottomLine"
                    app:layout_constraintEnd_toEndOf="@+id/breakfast26Guideline2"
                    android:gravity="center"
                    android:textColor="#838383"
                    android:textSize="14sp"
                    android:text="@string/Breakfast_26_venue"
                    android:onClick="venueClickEvent"/>

                <androidx.constraintlayout.widget.Group
                    android:id="@+id/breakfast26CardGroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:constraint_referenced_ids="breakfast26BottomLine,breakfast26Contents,breakfast26DressCode,breakfast26UpperLine,breakfast26Timings,breakfast26Image, breakfast26Venue"
                    android:visibility="gone"/>




            </androidx.constraintlayout.widget.ConstraintLayout>




        </androidx.cardview.widget.CardView>

